                                                                                    <input type="radio" name="rating" id="star1">
                                                                                    <label for="star1"><i class="ri-star-fill"></i></label>
                                                                                </div>
                                                                            </div>
                                                                            <form action="">
                                                                                <p>
                                                                                    <label>Name</label>
                                                                                    <input type="text">
                                                                                </p>

                                                                                <p>
                                                                                    <label>Summary</label>
                                                                                    <input type="text">
